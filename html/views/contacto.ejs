        <%- include('components/nav.ejs') %>
        <!-- end header -->
        <!-- start page title -->
        <section class="page-title-big-typography bg-dark-gray ipad-top-space-margin pb-30px" data-parallax-background-ratio="0.5" style="background-image: url(images/demo-conference-page-bg.jpg)">
            <div class="container">
                <div class="row align-items-center justify-content-center extra-small-screen">
                    <div class="col-lg-8 text-center page-title-extra-large">
                        <h1 class="mb-0 text-white alt-font fw-600 ls-minus-5px">
                            <span data-anime='{ "opacity": [0, 1], "easing": "easeOutQuad", "duration": 1000, "delay": 300 }'>Contacto</span>
                        </h1>
                    </div>
                </div>
            </div>
        </section>
        <!-- end page title -->
        <!-- start section --> 
        
        <!-- end section -->
        <!-- start section -->
        <section class="bg-midnight-blue pt-50px sm-pt-50px overflow-hidden">
            <div class="container"> 
                <div class="row row-cols-1 row-cols-lg-3 row-cols-md-2 justify-content-center" data-anime='{ "el": "childs", "translateX": [0, 0], "opacity": [0,1], "duration": 1200, "delay": 0, "staggervalue": 300, "easing": "easeOutQuad" }'>
                    <!-- start rotate box item -->
                    <div class="col rotate-box-style-02 md-mb-30px">
                        <div class="w-100 min-h-300px text-center rotate-box to-left">
                            <!-- start front side -->
                            <div class="w-100 h-100 overflow-hidden z-index-1 border-radius-5px front-side border border-color-transparent-white-light">
                                <div class="rotate-content-front z-index-2">
                                    <i class="line-icon-Geo2-Love icon-extra-large text-base-color mb-25px"></i>
                                    <div class="fs-20 text-dark mb-5px">Ubicación</div>
                                    <span class="w-60 lg-w-70 d-block m-auto">La pampa 2326, 409</span>
                                </div>
                            </div>
                            <!-- end front side -->
                            <!-- start back side -->
                            <div class="w-100 h-100 overflow-hidden border-radius-5px back-side cover-background">
                                <div class="opacity-light bg-charcoal-blue"></div>
                                <div class="d-flex flex-column align-items-center justify-content-center h-100 z-index-2 rotate-content-back p-15"> 
                                    <a href="https://www.google.com/maps/place/La+Pampa+2326,+C1428EAP+Cdad.+Aut%C3%B3noma+de+Buenos+Aires/@-34.5645455,-58.4560115,17z/data=!3m1!4b1!4m6!3m5!1s0x95bcb5d1bdec7f47:0x38e7c5dfbab07232!8m2!3d-34.5645499!4d-58.4534366!16s%2Fg%2F11fxdzl0t2?entry=ttu" target="_blank" class="btn btn-large btn-base-color btn-hover-animation btn-round-edge btn-box-shadow align-self-center">
                                        <span> 
                                            <span class="btn-text">Ver ubicación</span> 
                                            <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span> 
                                        </span>
                                    </a>
                                </div>
                                <!-- end back side -->
                            </div>
                        </div>    
                    </div>
                    <!-- end rotate box item -->   
                    <!-- start rotate box item -->
                    <div class="col rotate-box-style-02 md-mb-30px">
                        <div class="w-100 min-h-300px text-center rotate-box to-left">
                            <!-- start front side -->
                            <div class="w-100 h-100 overflow-hidden z-index-1 border-radius-5px front-side border border-color-transparent-white-light">
                                <div class="rotate-content-front z-index-2">
                                    <i class="line-icon-Headset icon-extra-large text-base-color mb-25px"></i>
                                    <div class="fs-20 text-dark mb-5px">Hablemos</div>
                                    <span class="d-block m-auto">+54 9 11 4980 4006</span>
                                </div>
                            </div>
                            <!-- end front side -->
                            <!-- start back side -->
                            <div class="w-100 h-100 overflow-hidden border-radius-5px back-side cover-background" >
                                <div class="opacity-light bg-charcoal-blue"></div>
                                <div class="d-flex flex-column align-items-center justify-content-center h-100 z-index-2 rotate-content-back p-15"> 
                                    
                                    <a href="https://wa.me/+5491149804006" class="btn btn-large btn-wpp-color btn-hover-animation btn-round-edge btn-box-shadow align-self-center">
                                        <span> 
                                            <span class="btn-text">Whatsapp</span> 
                                            <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span> 
                                        </span>
                                    </a>
                                </div>
                                <!-- end back side -->
                            </div>
                        </div>    
                    </div>
                    <!-- end rotate box item --> 
                    <!-- start rotate box item -->
                    <div class="col rotate-box-style-02">
                        <div class="w-100 min-h-300px text-center rotate-box to-left">
                            <!-- start front side -->
                            <div class="w-100 h-100 overflow-hidden z-index-1 border-radius-5px front-side border border-color-transparent-white-light">
                                <div class="rotate-content-front z-index-2">
                                    <i class="line-icon-Mail-Read icon-extra-large text-base-color mb-25px"></i>
                                    <div class="fs-20 text-dark mb-5px">Email</div> 
                                    <span class="d-block m-auto" id="textoEmail">jorge@red-futura.com</span> 
                                </div>
                            </div>
                            <!-- end front side -->
                            <!-- start back side -->
                            <div class="w-100 h-100 overflow-hidden border-radius-5px back-side cover-background" ">
                                <div class="opacity-light bg-charcoal-blue"></div>
                                <div class="d-flex flex-column align-items-center justify-content-center h-100 z-index-2 rotate-content-back p-15"> 
                                    <button class="btn btn-large btn-base-color btn-hover-animation btn-round-edge btn-box-shadow align-self-center" id="copiarEmailBtn">
                                        <span> 
                                            <span class="btn-text">Copiar Email</span> 
                                            <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span> 
                                        </span>
                                    </button>                      
                                </div>
                                <!-- end back side -->
                            </div>
                        </div>    
                    </div>
                    <!-- end rotate box item --> 
                </div>
            </div>
        </section>
        <!-- end section -->
        <!-- start section -->
        <section class="bg-midnight-blue xs-pt-50px pt-0">
            <div class="container">
                <div class="row justify-content-center mb-3 xs-mb-8" data-anime='{ "translateY": [50, 0], "opacity": [0,1], "duration": 600, "delay": 0, "staggervalue": 300, "easing": "easeOutQuad" }'>
                    <div class="col-xl-7 col-lg-8 text-center">
                        <h2 class="alt-font text-dark fw-500 ls-minus-2px"><span class="w-20px h-4px d-inline-block bg-base-color me-10px"></span>Estamos para ayudar</h2>
                    </div>
                </div>
                <div class="row row-cols-md-1 justify-content-center" data-anime='{ "translateY": [0, 0], "opacity": [0,1], "duration": 600, "delay": 100, "staggervalue": 300, "easing": "easeOutQuad" }'>
                    <div class="col-xl-9 col-lg-11">
                        <!-- start contact form -->
                        <form method="post" class="row contact-form-style-02" id="contact-form">
                            <div class="col-md-6 mb-30px">
                                <input class="input-name  form-control" type="text" name="name" placeholder="Nombre y Apellido*" id="name-form" required/>
                            </div>
                            <div class="col-md-6 mb-30px">
                                <input class=" form-control" type="email" name="email" placeholder="Email*" id="email-form" required/>
                            </div>
                            <div class="col-md-6 mb-30px">
                                <input class=" form-control" type="tel" name="phone" placeholder="Telefono*" id="phone-form" required/>
                            </div>
                            <div class="col-md-6 mb-30px">
                                <input class=" form-control" type="text" name="subject" name="subject" placeholder="Asunto" id="subject-form" required/>
                            </div>
                            <div class="col-md-12 mb-40px">
                                <textarea class=" form-control" cols="40" rows="4" name="comment" placeholder="Mensaje" id="message-form" required></textarea>
                            </div>
                            <div class="col-md-6">
                                <p class="mb-0 lh-30 text-center text-md-start fs-16">Estamos comprometidos a proteger su privacidad. Nunca recopilaremos información sobre usted sin su consentimiento explícito.</p>
                            </div>
                            <div class="col-md-3 text-center text-md-end sm-mt-20px">
                                <div class="g-recaptcha col-md-3" data-sitekey="6LeNlBgqAAAAAIHmAXGMGWppbrh3vewkq_aJJEVN"></div>
                            </div>
                            <div class="col-md-3 text-center text-md-end sm-mt-20px">
                                <input type="hidden" name="redirect" value="">
                                <button class="btn btn-medium btn-base-color btn-hover-animation btn-round-edge btn-box-shadow align-self-center" type="submit" id="submit-form">
                                    <span> 
                                        <span class="btn-text">Enviar</span> 
                                        <span class="btn-icon"><i class="fa-solid fa-arrow-right"></i></span> 
                                    </span>
                                </button>
                            </div>
                            <div class="col-12">
                                <div class="form-results mt-20px d-none"></div>
                            </div>
                        </form>
                        <!-- end contact form -->
                    </div>
                </div>
            </div>
        </section>
        <!-- end section -->
       
        <!-- start footer -->
        <%- include('components/footer.ejs') %>
        <!-- end footer -->
        <!-- start scroll progress -->
        <div class="scroll-progress d-none d-xxl-block">
            <a href="#" class="scroll-top" aria-label="scroll">
                <span class="scroll-text">Scroll</span><span class="scroll-line"><span class="scroll-point"></span></span>
            </a>
        </div>
        <!-- end scroll progress -->
        <!-- javascript libraries -->
        <script src="https://cdn.emailjs.com/dist/email.min.js"></script>
        <script src="https://www.google.com/recaptcha/api.js" async defer></script>
        <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
        <script type="text/javascript" src="js/nav.js"></script>
        <script type="text/javascript" src="js/jquery.js"></script>
        <script type="text/javascript" src="js/vendors.min.js"></script>
        <script type="text/javascript" src="js/main.js"></script>
        <script type="text/javascript" src="js/form.js"></script>   
    </body>
</html>

<script>
    document.getElementById('copiarEmailBtn').addEventListener('click', function() {
    const texto = document.getElementById('textoEmail').textContent;
    navigator.clipboard.writeText(texto).then(() => {
        alert('Texto copiado al portapapeles');
    }).catch(err => {
        console.error('Error al copiar el texto: ', err);
    });
});
</script>